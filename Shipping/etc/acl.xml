<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="BlueEx_Shipping::shipping" title="Bluex Shipping" translate="title" sortOrder="40">
                    <resource id="BlueEx_Shipping::citylist" title="City List" translate="title" sortOrder="10" />
                    <resource id="BlueEx_Shipping::tariff" title="Tariff" translate="title" sortOrder="10" />
                    <resource id="BlueEx_Shipping::settings" title="Settings" translate="title" sortOrder="10" />
                    <resource id="BlueEx_Shipping::updateproductapi" title="Update ProductQty Api" translate="title" sortOrder="10" />
                </resource>
                <resource id="Magento_Sales::sales">
                    <resource id="Magento_Sales::sales_operation">
                        <resource id="Magento_Sales::sales_order">
                            <resource id="Magento_Sales::actions">
                                <resource id="Magento_Sales::delete" title="Delete" translate="title" sortOrder="65"/>
                            </resource>
                        </resource>
                    </resource>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="BlueEx_Shipping::configuration" title="Mageplaza Delete Orders"/>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
